{

    "everymine": "",
    "channelID": "",

    "show": {
        "enter": true,
        "leave": true,
        "join": true,
        "advancement": true
    },

    "template": {

        "bot_discord": "[%text%]",
        "discord": "`%username%`: %message%",
        "tellraw": {

            "message": [{ "color": "white", "text": "%message%" }],

            "user": [
                { "color": "white", "text": "<" },
                {
                    "color": "gray",
                    "text": "%username%#%discriminator%",
                    "hoverEvent": {
                        "action": "show_text"
                    }
                },
                { "color": "white", "text": ">%bot% " }
            ],

            "url": [{
                "color": "white",
                "underlined": true,

                "clickEvent": {
                    "action": "open_url"
                }
            }]

        }

    }

}